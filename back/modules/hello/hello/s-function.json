{
  "functions": {
    "hello": {
      "custom": {
        "excludePatterns": [],
        "envVars": []
      },
      "handler": "modules/hello/hello/handler.handler",
      "timeout": 6,
      "memorySize": 128,
      "eventSourceArn": "",
      "endpoints": [
        {
          "path": "hello/hello",
          "method": "GET",
          "authorizationType": "none",
          "apiKeyRequired": false,
          "requestParameters": {},
          "requestTemplates": {
            "application/json": ""
          },
          "responses": {
            "400": {
              "statusCode": "400"
            },
            "default": {
              "statusCode": "200",
              "responseParameters": {
                "method.response.header.Content-Type": "'text/html'" 
              },
              "responseModels": {},
              "responseTemplates": {
                "text/html": "$input.path('$')"
              }
            }
          }
        }
      ]
    }
  }
}
